{
  "version": 1,
  "paths": {
    "/customers/{customerId}": {
      "name": "/customers/{customerId}",
      "lambdaFunction": "superHandlerApi",
      "permissions": {
        "setting": "open"
      }
    },
    "/vendas": {
      "name": "/vendas",
      "lambdaFunction": "superHandlerApi",
      "permissions": {
        "setting": "open"
      }
    },
    "/products": {
      "name": "/products",
      "lambdaFunction": "superHandlerApi",
      "permissions": {
        "setting": "open"
      }
    },
    "/users": {
      "name": "/users",
      "lambdaFunction": "superHandlerApi",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}